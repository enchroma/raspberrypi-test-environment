  "keywords": [
    "budo",
    "browserify",
    "choo",
    "postcss"
  ],
  "repository": {
    "type": "git",
    "url": ".git",
    "web": "/"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "choo": "^6.7.0",
    "choo-log": "^8.0.0",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "fastclick": "^1.0.6",
    "ip": "^1.1.5",
    "lodash": "^4.17.4",
    "path-parse": "^1.0.5",
    "python-shell": "^0.5.0",
    "readdir": "0.0.13",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.1.0",
    "xhr-request": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "budo": "^11.2.0",
    "concurrently": "^3.5.1",
    "cssnano-cli": "^1.0.5",
    "ejs-cli": "^2.0.0",
    "envify": "^4.1.0",
    "escodegen": "^1.9.0",
    "estraverse": "^4.2.0",
    "normalize.css": "^8.0.0",
    "postcss-cli": "^4.1.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-properties": "^6.2.0",
    "postcss-fontpath": "^1.0.0",
    "postcss-import": "^11.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "uglify-js": "^3.3.15"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      [
        "envify"
      ]
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development ejs-cli src/index.ejs > public/static/index.html && NODE_ENV=development concurrently \"budo src/js/app.js:bundle.js --pushstate --live --dir public --serve bundle.js \" \"postcss -c postcss.config.js src/postcss/index.css -o public/static/main.css -m -w\" ",
    "build:socket": "NODE_ENV=production browserify src/socket.js | uglifyjs > public/static/socket.min.js",
    "build": "concurrently \"chmod +x bin/generate-files.sh && bin/generate-files.sh\" \"NODE_ENV=production browserify src/js/app.js | uglifyjs > public/static/bundle.min.js \" \" postcss -c postcss.config.js src/postcss/index.css -o public/static/main.css && cssnano < public/static/main.css > public/static/main.min.css && NODE_ENV=production ejs-cli src/index.ejs > public/static/index.html\" \"npm run build:socket\" "
  },
  "false": {}
}
